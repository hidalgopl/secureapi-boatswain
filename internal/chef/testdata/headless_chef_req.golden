{
  "metadata": {
    "name": "chef-fake-uuid",
    "creationTimestamp": null,
    "labels": {
      "podName": "chef-fake-uuid",
      "app": "chef",
      "chef-type": "headless"
    },
    "annotations": {
      "sumologic.io/exclude": "true"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "chef",
        "image": "chefImage:chrome-72-chefTag",
        "ports": [
          {
            "containerPort": 5995
          }
        ],
        "env": [
          {
            "name": "HEADLESS_CHEF",
            "value": "True"
          },
          {
            "name": "CONTAINERIZED_CHEF",
            "value": "True"
          },
          {
            "name": "DRYADS",
            "value": "dryad:latest"
          }
        ],
        "resources": {
          "limits": {
            "cpu": "1",
            "memory": "2Gi"
          },
          "requests": {
            "cpu": "200m",
            "memory": "200Mi"
          }
        }
      }
    ],
    "nodeSelector": {
      "chefonly": "true"
    },
    "imagePullSecrets": [
      {
        "name": "quaypull"
      }
    ],
    "tolerations": [
      {
        "key": "chefonly",
        "operator": "Equal",
        "value": "true",
        "effect": "NoSchedule"
      }
    ]
  },
  "status": {}
}