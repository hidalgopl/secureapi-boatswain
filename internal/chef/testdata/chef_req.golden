{
  "metadata": {
    "name": "chef-fake-uuid",
    "creationTimestamp": null,
    "labels": {
      "podName": "chef-fake-uuid",
      "app": "chef"
    },
    "annotations": {
      "sumologic.io/exclude": "true"
    }
  },
  "spec": {
    "volumes": [
      {
        "name": "browsers",
        "persistentVolumeClaim": {
          "claimName": "chef-browser-storage"
        }
      }
    ],
    "initContainers": [
      {
        "name": "browser-sync",
        "image": "browser-sync-image",
        "env": [
          {
            "name": "BROWSER",
            "value": "chrome"
          },
          {
            "name": "VERSION",
            "value": "72.0.3626.121"
          }
        ],
        "resources": {},
        "volumeMounts": [
          {
            "name": "browsers",
            "mountPath": "/browsers"
          }
        ]
      }
    ],
    "containers": [
      {
        "name": "chef",
        "image": "chef-image:chef-tag",
        "ports": [
          {
            "containerPort": 5995
          }
        ],
        "env": [
          {
            "name": "KUBE_CHEF",
            "value": "True"
          },
          {
            "name": "CONTAINERIZED_CHEF",
            "value": "True"
          }
        ],
        "resources": {
          "limits": {
            "cpu": "1",
            "memory": "2Gi"
          },
          "requests": {
            "cpu": "200m",
            "memory": "200Mi"
          }
        },
        "volumeMounts": [
          {
            "name": "browsers",
            "readOnly": true,
            "mountPath": "/browsers"
          }
        ],
        "readinessProbe": {
          "exec": {
            "command": [
              "test",
              "-f",
              "/tmp/ready",
              "-a",
              "-d",
              "/browsers/chrome-72"
            ]
          },
          "periodSeconds": 1
        }
      }
    ],
    "nodeSelector": {
      "chefonly": "true"
    },
    "imagePullSecrets": [
      {
        "name": "quaypull"
      }
    ],
    "tolerations": [
      {
        "key": "chefonly",
        "operator": "Equal",
        "value": "true",
        "effect": "NoSchedule"
      }
    ]
  },
  "status": {}
}